/* Update driver no android */
  appium driver update uiautomator2 --unsafe


/* Update driver do ios */
  appium driver update xcuitest --unsafe


/* Update appium */
    npm i --location=global appium

/* Version android */
    adb shell getprop ro.build.version.release

